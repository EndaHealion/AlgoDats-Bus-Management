
import java.util.HashMap;

public class BusStopSearch {

	public class TrieNode {
		public HashMap<Character, TrieNode> children;
		public String value;
		public boolean isEnd;

		void addChild(char c, TrieNode node) {
			children.put((Character) c, node);
		}

		TrieNode getChild(char c) {
			return children.get((Character) c);
		}

		TrieNode() {
			children = new HashMap<Character, TrieNode>();
			value = "";
			isEnd = false;
		}
	}

	public class Trie {
		public final int RADIX = 26;

		public TrieNode root = null;

		public void insert(String word) {
			if (root == null) {
				return;
			}

			TrieNode node = root;
			for (int i = 0; i < word.length(); i++) {
				char currentChar = word.charAt(i);
				if (!node.children.containsKey((Character) currentChar)) {
					node.children.put((Character) currentChar, new TrieNode());
				}
				node = node.children.get((Character) currentChar);
			}
			node.isEnd = true;
		}

		public TrieNode searchPrefix(String word) {
			if (root == null) {
				return null;
			}

			TrieNode node = root;
			for (int i = 0; i < word.length(); i++) {
				char curLetter = word.charAt(i);
				if (node.children.containsKey(curLetter)) {
					node = node.children.get(curLetter);
				}
				else {
					return null;
				}
			}
			return node;
		}

	}

}
